package indexer;

public class ByteMaker {

	public void byteConverter(char a){
		
		
		int code = java.lang.Character.getNumericValue(a);
		
		System.out.println(a);
		
	}
	
	
	
	
	
	
	
public static void main(String[] args) {
	ByteMaker test = new ByteMaker();
	
	test.byteConverter('a');
	
	
}	
	
}
