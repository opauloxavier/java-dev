package indexer;

public class ByteMaker {

	public void byteConverter(char a){
		
		int code = (int) a;
		
		System.out.println(code);
		
		int b = code;
		char c = (char) b;
		
		System.out.println(c);
	}
	
	public void byteConverter(int a){
		
		int code = (int) a;
		
		System.out.println(code);
		
		int b = code;
		char c = (char) b;
		
		System.out.println(c);
	}
	
	public void binaryConverter(int code){
		
		
		
	}
	
	
	
	
	
	
public static void main(String[] args) {
	ByteMaker test = new ByteMaker();
	
	test.byteConverter('#');
	
	
}	
	
}
